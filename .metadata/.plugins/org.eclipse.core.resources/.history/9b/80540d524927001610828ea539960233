package fr.actia.utilitaire;

import java.io.File;

public class DeleteDirectories {

	public static void main(String[] args) {
		
		File myFolder = new File("/home/yassine/Developpement/Repositories/Bonita_REST/TestRest/tmp/ticket_11/final");
	
		
		if(myFolder.exists())
		{
			System.out.println("existe");
			myFolder.delete();
		}
		if(!myFolder.exists())
		{
			System.out.println("n'existe plus");
		}

	}

}
