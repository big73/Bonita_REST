package fr.actia.utilitaire;

import java.io.File;


public class DeleteDirectories {

	public static void main(String[] args) {
		
		File myFolder = new File("/home/yassine/Developpement/Repositories/Bonita_REST/TestRest/tmp/ticket_9/final/context.xml");
	
		if(myFolder.exists())
		{
			System.out.println("existe");
			myFolder.delete();
		}
		else
		{
			System.out.println("n'existe pas");
		}
		
		
		
	}

}
